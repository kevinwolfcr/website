    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#696e77",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#adb1b8"
      }
    },
    {
      "scope": ["variable", "string", "punctuation.definition.variable"],
      "settings": {
        "foreground": "#edeef0"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#edeef0"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#49dfbe"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "punctuation.definition.function"],
      "settings": {
        "foreground": "#49dfbe"
      }
    },
    {
      "scope": ["text", "source"],
      "settings": {
        "foreground": "#adb1b8"
      }
    },
    {
